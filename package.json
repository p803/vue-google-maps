        "eslint": "^6.7.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-vue": "^6.2.2",
        "rollup": "^2.10.5",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-terser": "^5.3.0",
        "rollup-plugin-vue": "^5.1.7",
        "vue": "^2.6.11",
