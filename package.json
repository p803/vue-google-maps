{
    "name": "@p803/vue-google-maps",
    "version": "2.1.0",
    "description": "Google Maps plugin for Vue",
    "title": "Vue Google Maps",
    "main": "dist/index.umd.js",
    "module": "dist/index.esm.js",
    "unpkg": "dist/index.min.js",
    "scripts": {
        "build": "rm --recursive --force dist && mkdir dist && rollup --config",
        "build:doc": "vuepress build doc",
        "lint": "vue-cli-service lint .",
        "prepublishOnly": "npm run build && npm run build:doc",
        "postpublish": "node bin/publish-repository",
        "serve:example": "vue-cli-service serve example",
        "serve:doc": "vuepress dev doc",
        "test:unit": "vue-cli-service test:unit"
    },
    "gitHooks": {
        "pre-commit": "npm run lint && git add . && npm run test:unit"
    },
    "files": [
        "dist"
    ],
    "bugs": {
        "url": "https://github.com/p803/vue-google-maps/issues"
    },
    "homepage": "https://github.com/p803/vue-google-maps#readme",
    "keywords": [
        "vue",
        "google-maps"
    ],
    "author": "≈Åukasz Proksa",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/p803/vue-google-maps.git"
    },
    "dependencies": {
        "@babel/runtime": "^7.9.6",
        "@p803/google-maps-api": "^2.1.0",
        "core-js": "^3.8.1"
    },
    "devDependencies": {
        "@rollup/plugin-alias": "^3.1.1",
        "@rollup/plugin-babel": "^5.2.2",
        "@rollup/plugin-commonjs": "^11.1.0",
        "@rollup/plugin-node-resolve": "^7.1.3",
        "@vue/cli-plugin-babel": "^4.3.0",
        "@vue/cli-plugin-eslint": "^4.3.0",
        "@vue/cli-plugin-unit-jest": "^4.3.0",
        "@vue/cli-service": "^4.3.0",
        "@vue/eslint-config-airbnb": "^5.0.2",
        "@vue/test-utils": "1.0.0-beta.31",
        "babel-eslint": "^10.1.0",
        "dotenv-flow": "^3.1.0",
        "eslint": "^6.7.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-vue": "^6.2.2",
        "rollup": "^2.34.2",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-vue": "^5.1.7",
        "vue": "^2.6.11",
        "vue-template-compiler": "^2.6.11",
        "vuepress": "^1.5.0"
    }
